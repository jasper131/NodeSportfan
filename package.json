{
  "name": "sportfan",
  "version": "0.0.0",
  "description": "FifaFan is a platform that connect football players, fans, personnel",
  "private": true,
  "scripts": {
    "lint": "eslint --fix \"./src/**/*.js\"",
    "test": "cross-env NODE_ENV=test npm run db:migrate:undo && npm run db:migrate && nyc mocha --no-timeout './tests/index.js' --exit",
    "test:report": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha --timeout 5000 './tests/index.js' --exit",
    "dev": "nodemon --exec babel-node ./src/index.js",
    "start:dev": "npm run migrate && nodemon --exec babel-node ./src/index.js",
    "start": "npm run build && node build/index.js",
    "build": "babel src --out-dir build",
    "migrate": "sequelize db:migrate ",
    "migrate:reset": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "migrate:seed": "npm run migrate && npm run seed",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset && cross-env NODE_ENV=test npm run migrate && cross-env NODE_ENV=test npm run seed",
    "prestart": "npm run migrate",
    "db:generate:model": "npx sequelize model:generate",
    "db:seed:undo": "npx sequelize db:seed:undo",
    "coverage": "cross-env NODE_ENV=test nyc npm run test && nyc report --reporter=lcov --reporter=text-lcov --exit",
    "test-a": "cross-env NODE_ENV=test nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Next-Inn/FifaFans.git"
  },
  "author": "Tueloper, Lovisgod, Promise, Ogochukwu",
  "license": "ISC",
  "homepage": "https://github.com/Next-Inn/FifaFans#readme",
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/compat-data": "^7.12.7",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "canvas": "^2.6.1",
    "cloudinary": "^1.23.0",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "datauri": "^3.0.0",
    "debug": "~4.3.1",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "~4.16.1",
    "express-handlebars": "^5.2.0",
    "google-auth-library": "^6.1.4",
    "hbs": "^4.1.1",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "newsapi": "^2.4.1",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "redis": "^3.1.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "socket.io": "^2.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/compat-data": "^7.12.7",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.1.0",
    "mocha": "^8.2.1",
    "nodemailer-mock": "^1.5.3",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "exclude": [
      "src/models/index.js",
      "src/models",
      "tests",
      "node_modules"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "cache": false,
    "report-dir": "tests/report"
  }
}
